{
  "name": "xtype",
  "version": "1.0.0",
  "description": "express with typescript",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run tsc-client:w\" \"npm run nmon\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w -p ./server",
    "tsc-client:w": "tsc -w -p ./client",
    "nmon": "node server/build/app.js",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "keywords": [
    "expressjs",
    "typescript",
    "node",
    "es6"
  ],
  "author": "Vildan Tursic",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "express-brute": "^0.6.0",
    "express-jwt": "^3.3.0",
    "express-unless": "^0.3.0",
    "json-stream": "^1.0.0",
    "jsonwebtoken": "^5.7.0",
    "mongoose": "^4.4.12",
    "nodemon": "^1.9.1",
    "socket.io": "^1.4.5",
    "angular2": "2.0.0-beta.12",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "socket.io-client": "^1.4.5",
    "systemjs": "0.19.24",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "concurrently": "^2.0.0",
    "tslint": "^3.7.1",
    "typescript": "^1.8.9",
    "typings": "^0.7.12"
  }
}
